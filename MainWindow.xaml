<Window x:Class="hashTable.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:hashTable"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="txtKey" HorizontalAlignment="Left" Height="23" Margin="137,81,0,0" TextWrapping="Wrap" Text="Key" VerticalAlignment="Top" Width="256"/>
        <TextBox x:Name="txtValue" HorizontalAlignment="Left" Height="23" Margin="137,115,0,0" TextWrapping="Wrap" Text="Value" VerticalAlignment="Top" Width="256"/>
        <Button x:Name="btnAdd" Content="Add" HorizontalAlignment="Left" Margin="137,150,0,0" VerticalAlignment="Top" Width="75" Click="btnAdd_Click"/>
        <Button x:Name="btnRemove" Content="Remove" HorizontalAlignment="Left" Margin="226,150,0,0" VerticalAlignment="Top" Width="76" Click="btnRemove_Click"/>
        <Button x:Name="btnShow" Content="ShowAll" HorizontalAlignment="Left" Margin="317,150,0,0" VerticalAlignment="Top" Width="76" Click="btnShow_Click"/>

    </Grid>
</Window>
